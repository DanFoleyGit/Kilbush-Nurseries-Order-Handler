            android:screenOrientation="portrait"
            android:configChanges="orientation"
            />
        <activity
            android:name=".LidlCalculatorActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"
            />
        <activity
            android:name=".LidlOrdersActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"
            />
        <activity
            android:name=".TescoCalculatorActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"
            />
        <activity
            android:name=".TescoOrdersActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation"
            />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>